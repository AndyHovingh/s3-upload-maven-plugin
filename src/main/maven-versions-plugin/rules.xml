<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0
         http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd
         http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 ">
    <rules>
        <rule groupId="org.apache.maven.plugins">
            <ignoreVersions>
                <ignoreVersion type="regex">(?i).*jboss-.+</ignoreVersion>
                <ignoreVersion type="regex">(?i).*jenkins-.+</ignoreVersion>
                <ignoreVersion type="regex">(?i).*M(?:-?\d+)?</ignoreVersion><!-- milestones -->
            </ignoreVersions>
        </rule>
        <rule groupId="com.fasterxml.jackson">
            <ignoreVersions>
                <ignoreVersion type="regex">(?i).*.pr\d+</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
